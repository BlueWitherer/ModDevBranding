{
    "geode": "4.10.0",
    "gd": {
        "win": "2.2074",
        "android": "2.2074",
        "mac": "2.2074",
        "ios": "2.2074"
    },
    "id": "cheeseworks.moddevbranding",
    "name": "Mod Developer Branding",
    "version": "1.0.0",
    "developers": [
        "Cheeseworks"
    ],
    "description": "Developer branding on mod info popups.",
    "tags": [
        "enhancement"
    ],
    "dependencies": {
        "geode.node-ids": {
            "importance": "required",
            "version": ">=1.21.0"
        },
        "alphalaneous.alphas_geode_utils": {
            "importance": "required",
            "version": ">=1.1.0"
        },
        "prevter.imageplus": {
            "importance": "recommended",
            "version": ">=1.0.0"
        }
    },
    "links": {
        "source": "https://github.com/BlueWitherer/ModDevBranding",
        "homepage": "https://moddev.cheeseworks.gay/",
        "community": "https://www.dsc.gg/cubic"
    },
    "issues": {
        "info": "Report any issues to my repository!",
        "url": "https://www.github.com/BlueWitherer/ModDevBranding/issues/"
    },
    "api": {
        "include": [
            "include/*.hpp"
        ]
    },
    "settings": {
        "preview-image": {
            "type": "file",
            "name": "Test Brand Image",
            "description": "Upload an image to test it out as branding on this mod's popup.",
            "default": "",
            "control": {
                "dialog": "open",
                "filters": [
                    {
                        "files": [
                            "*.png",
                            "*.jpg",
                            "*.jpeg",
                            "*.webp",
                            "*.gif"
                        ]
                    }
                ]
            }
        },
        "timeout": {
            "type": "float",
            "name": "Load Timeout",
            "description": "Duration in seconds until the branding image cancels loading.",
            "default": 5.0,
            "min": 1.0,
            "max": 45.0,
            "control": {
                "input": true,
                "slider": true,
                "slider-step": 1.0,
                "arrows": true,
                "arrow-step": 5.0,
                "big-arrows": true,
                "big-arrow-step": 15.0
            }
        }
    },
    "early-load": false
}